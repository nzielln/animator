            - if transformation == "color"
            - if transformation == "height"; if shape is oval  == "yRadius"
            - if transformation == "width"; if shape is oval  == "xRadius"
            - if transformation == "color"
            - if transformation == "height"
        - changePosition
        - changeColor
        - changeHeight
        - changeWidth
        - changeXRadius
        - changeYRadius
        - getAppears
